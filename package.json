    "config": "^1.25.1",
    "crypto": "^1.0.1",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "eslint": "^3.17.1",
    "express": "^4.15.2",
    "express-flash": "0.0.2",
